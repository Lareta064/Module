					.swiper-slide
						img(src="./img/image2.webp",width="453" height="288" alt="img")
					.swiper-slide
						img(src="./img/image1.webp",width="453" height="288" alt="img")
					.swiper-slide
						img(src="./img/image1.webp",width="453" height="288" alt="img")
			
